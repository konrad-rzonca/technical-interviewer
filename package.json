{
  "name": "technical-interview-platform",
  "version": "1.0.0",
  "description": "A React application for conducting technical interviews",
  "main": "src/index.js",
  "homepage": "/technical-interviewer",
  "scripts": {
    "start": "react-scripts start",
    "start:base": "cross-env REACT_APP_THEME=base react-scripts start",
    "start:ubs": "cross-env REACT_APP_THEME=ubs react-scripts start",
    "start:ubs-chrome-optimized": "cross-env REACT_APP_THEME=ubs npm run start -- --openssl-legacy-provider && open -a \"Google Chrome\" --args --disable-background-timer-throttling --disable-backgrounding-occluded-windows --disable-renderer-backgrounding",
    "build": "react-scripts build",
    "build:base": "cross-env REACT_APP_THEME=base react-scripts build",
    "build:ubs": "cross-env REACT_APP_THEME=ubs react-scripts build",
    "build:ubs-optimized": "cross-env REACT_APP_THEME=ubs GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mui/icons-material": "^6.4.7",
    "@mui/lab": "^6.0.0-beta.30",
    "@mui/material": "^6.4.7",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.0",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.0",
    "react-scripts": "5.0.1",
    "react-virtualized-auto-sizer": "^1.0.25",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "cross-env": "^7.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
